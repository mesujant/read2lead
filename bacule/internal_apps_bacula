applied firewall rules:

watch -n1 'cat hosts_and_networks | tail -5; echo "#####";cat ports | tail -5; echo "###########";cat /usr/admin/AppliedFirewall/rules/allow.in_tcp_ports_from_hosts | tail -2;'

cp -r /usr/admin/AppliedFirewall /home/sujant/;
echo 'HOST_BACULA_FLOAT="10.12.10.7"' >> /usr/admin/AppliedFirewall/definitions/hosts_and_networks;
echo 'HOST_BACULA_DIR="10.12.10.8"'   >> /usr/admin/AppliedFirewall/definitions/hosts_and_networks;
echo 'HOST_BACULA_DIR_HA="10.12.10.9"' >> /usr/admin/AppliedFirewall/definitions/hosts_and_networks;
echo 'HOST_BACULA_SD="202.79.37.59"'   >> /usr/admin/AppliedFirewall/definitions/hosts_and_networks;
echo 'PORT_BACULA_DIR="9102"' >> /usr/admin/AppliedFirewall/definitions/ports;
echo 'PORT_BACULA_SD="9103"' >> /usr/admin/AppliedFirewall/definitions/ports;
echo 'IN_BACULA_DIR="BACULA_FLOAT BACULA_DIR BACULA_DIR_HA"'  >> /usr/admin/AppliedFirewall/rules/allow.in_tcp_ports_from_hosts; 
echo 'IN_BACULA_SD="BACULA_SD"' >> /usr/admin/AppliedFirewall/rules/allow.in_tcp_ports_from_hosts;
cd /usr/admin/AppliedFirewall;
./AppliedFirewall reload inbound;
service iptables save;



dpkg -r bacula-client bacula-common;
dpkg --purge bacula-client bacula-common;

iptables-save > /home/sujant/iptables-$(date +%Y-%m-%d);
iptables -I INPUT 3 -s 10.12.10.0/24 -p tcp --dport 9102 -j ACCEPT -m comment --comment "bacula";



